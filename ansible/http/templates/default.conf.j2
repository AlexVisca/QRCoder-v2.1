server {
    listen       80;
    server_name  localhost;

    #access_log  /var/log/nginx/host.access.log  main;

    location / {
        root   /usr/share/nginx/html;
        index  index.html index.htm;
    }

	location /final {
        root {{ http_static_dir }};
		proxy_pass {{ application_host}}:{{ application_port }};
	}
}
